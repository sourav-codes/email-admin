{
  "name": "react-web-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f .env.dev react-scripts start",
    "start:qa": "env-cmd -f .env.qa react-scripts start",
    "start:stage": "env-cmd -f .env.staging react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd -f .env.dev react-scripts build",
    "build:stage": "env-cmd -f .env.staging react-scripts build",
    "build:qa": "env-cmd -f .env.qa react-scripts build",
    "build:prod": "env-cmd -f .env.prod react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier:write": "npx prettier --write **/*.{js,jsx,json} && npx prettier --write *.{js,jsx,json}"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "^4.12.3",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "formik": "^2.2.9",
    "global": "^4.4.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "sass": "^1.57.1",
    "web-vitals": "^3.1.0",
    "yup": "^0.32.11"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "prettier": "2.8.1"
  }
}
